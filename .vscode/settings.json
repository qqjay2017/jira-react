{
  "cSpell.words": [
    "ahooks",
    "antd",
    "Bearer",
    "browserslist",
    "commitlint",
    "craco",
    "idel",
    "middlewares",
    "UnauthenicationApp"
  ],
  "files.autoSave": "off",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],

  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
